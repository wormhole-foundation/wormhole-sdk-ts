{
  "name": "@wormhole-foundation/connect-sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/connect-sdk/issues"
  },
  "homepage": "https://github.com/wormhole-foundation/connect-sdk#readme",
  "directories": {
    "test": "tests"
  },
  "license": "Apache-2.0",
  "version": "0.1.0-beta.1",
  "type": "module",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "types": "./cjs/index.d.ts",
      "require": "./cjs/index.js",
      "import": "./esm/index.js",
      "default": "./esm/index.js"
    }
  },
  "author": "",
  "description": "The core package for the Connect SDK, used in conjunction with 1 or more of the chain packages",
  "files": [
    "cjs/**/*",
    "esm/**/*",
    "src/**/*"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json",
    "clean": "rimraf cjs esm",
    "lint": "npm run prettier && eslint --fix",
    "prettier": "prettier --write ./src",
    "test": "jest",
    "coverage": "jest --coverage",
    "docs":"typedoc"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.14",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@nomad-xyz/multi-provider": "^1.1.0",
    "binary-parser": "^2.2.1",
    "elliptic": "^6.5.4",
    "ethers": "^5.7.2"
  }
}
