{
  "name": "@wormhole-foundation/sdk-aptos-core",
  "version": "0.5.0-alpha.7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/connect-sdk/issues"
  },
  "homepage": "https://github.com/wormhole-foundation/connect-sdk#readme",
  "directories": {
    "test": "__tests__"
  },
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "description": "SDK for Solana, used in conjunction with @wormhole-foundation/connect-sdk",
  "files": [
    "dist/esm",
    "dist/cjs"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect",
    "aptos"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": true,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json  && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:esm && npm run build:cjs",
    "rebuild": "npm run clean && npm run build",
    "clean": "rm -rf ./dist && rm -rf ./.turbo",
    "lint": "npm run prettier && eslint --fix ./src --ext .ts",
    "prettier": "prettier --write ./src"
  },
  "devDependencies": {
    "nock": "^13.3.3"
  },
  "dependencies": {
    "@wormhole-foundation/sdk-connect": "0.5.0-alpha.7",
    "@wormhole-foundation/sdk-aptos": "0.5.0-alpha.7"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.js"
    }
  }
}